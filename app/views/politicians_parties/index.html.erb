<p id="notice"><%= notice %></p>

<h1>Politicians Parties</h1>

<table>
  <thead>
    <tr>
      <th>Politic party</th>
      <th>Politician rol</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @politicians_parties.each do |politicians_party| %>
      <tr>
        <td><%= politicians_party.politic_party %></td>
        <td><%= politicians_party.politician_rol %></td>
        <td><%= politicians_party.active %></td>
        <td><%= link_to 'Show', politicians_party %></td>
        <td><%= link_to 'Edit', edit_politicians_party_path(politicians_party) %></td>
        <td><%= link_to 'Destroy', politicians_party, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

